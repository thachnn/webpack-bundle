{
  "version": "6.14.18-4b",
  "name": "npm",
  "description": "a package manager for JavaScript",
  "keywords": [
    "install",
    "modules",
    "package manager",
    "package.json"
  ],
  "preferGlobal": true,
  "config": {
    "publishtest": false
  },
  "homepage": "https://docs.npmjs.com/",
  "author": "Isaac Z. Schlueter <i@izs.me> (http://blog.izs.me)",
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/cli"
  },
  "bugs": {
    "url": "https://npm.community/c/bugs"
  },
  "directories": {
    "bin": "./bin",
    "doc": "./docs",
    "lib": "./lib",
    "man": "./man"
  },
  "main": "./lib/npm.js",
  "bin": {
    "npm": "./bin/npm-cli.js",
    "npx": "./bin/npx-cli.js"
  },
  "overrides": {
    "boxen": {
      "term-size": "^2.2.0"
    },
    "http-proxy-agent": {
      "debug": "^3.1.0"
    }
  },
  "license": "Artistic-2.0",
  "engines": {
    "node": "6 >=6.2.0 || 8 || >=9.3.0"
  }
}