{
  "version": "8.19.4-2b",
  "name": "npm",
  "description": "a package manager for JavaScript",
  "files": [
    "bin/",
    "lib/",
    "index.js",
    "docs/content/",
    "docs/output/",
    "man/"
  ],
  "keywords": [
    "install",
    "modules",
    "package manager",
    "package.json"
  ],
  "preferGlobal": true,
  "config": {
    "publishtest": false
  },
  "homepage": "https://docs.npmjs.com/",
  "author": "GitHub Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/cli.git"
  },
  "bugs": {
    "url": "https://github.com/npm/cli/issues"
  },
  "directories": {
    "bin": "./bin",
    "doc": "./docs",
    "lib": "./lib",
    "man": "./man"
  },
  "main": "./index.js",
  "bin": {
    "npm": "bin/npm-cli.js",
    "npx": "bin/npx-cli.js"
  },
  "exports": {
    ".": [
      {
        "default": "./index.js"
      },
      "./index.js"
    ],
    "./package.json": "./package.json"
  },
  "license": "Artistic-2.0",
  "engines": {
    "node": "^12.13.0 || ^14.15.0 || >=16.0.0"
  }
}