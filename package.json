{
  "name": "webpack-bundle",
  "version": "1.0.0",
  "description": "Bundle NPM packages using webpack",
  "keywords": [
    "npm",
    "package",
    "bundle",
    "webpack",
    "module"
  ],
  "repository": "thachnn/webpack-bundle",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "v8-compile-cache": "2"
  },
  "scripts": {
    "build": "node scripts/build-webpack.js",
    "clean": "node node_modules/rimraf/bin.js dist/ node_modules/.cache",
    "webpack": "node node_modules/webpack-cli/bin/cli.js --progress"
  },
  "devDependencies": {
    "npm": "thachnn/npm#release-8.19.4",
    "rimraf": "^2.2.8",
    "string-replace-loader": "^3.0.0",
    "webpack": "thachnn/webpack-bundle#v5.74.0",
    "webpack-cli": "thachnn/webpack-bundle#cli-4.10.0"
  },
  "resolutions": {
    "**/schema-utils/ajv": "thachnn/webpack-bundle#ajv-6.12.6"
  },
  "overrides": {
    "schema-utils": {
      "ajv": "thachnn/webpack-bundle#ajv-6.12.6"
    }
  }
}
