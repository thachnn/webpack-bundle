{
  "name": "webpack-bundle",
  "version": "1.0.0",
  "description": "Bundle NPM packages using webpack",
  "keywords": [
    "npm",
    "package",
    "bundle",
    "webpack",
    "module"
  ],
  "repository": "thachnn/webpack-bundle",
  "license": "MIT",
  "engines": {
    "node": ">=6.9"
  },
  "scripts": {
    "postinstall": "git apply --reject --unidiff-zero scripts/webpack-deps.patch",
    "build": "node scripts/build-webpack.js",
    "clean": "node node_modules/rimraf/bin.js dist/ node_modules/.cache"
  },
  "devDependencies": {
    "rimraf": "^2.2.8",
    "webpack": "thachnn/webpack-bundle#v4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "resolutions": {
    "**/define-property": "^2.0.2",
    "**/chalk/supports-color": "^6.1.0",
    "**/resolve-dir/global-modules": "^2.0.0",
    "**/unset-value/has-value": "^1.0.0"
  },
  "overrides": {
    "define-property": "^2.0.2",
    "chalk": {
      "supports-color": "^6.1.0"
    },
    "resolve-dir": {
      "global-modules": "^2.0.0"
    },
    "unset-value": {
      "has-value": "^1.0.0"
    }
  }
}
